<div class="row">
	<p id="notice"><%= notice %></p>

	<h1>Listing Zips</h1>
	<%= link_to 'New Zip', new_zip_path  %> <br>
	<div class="table-responsive">
	 <table class="table table-hover">
	  <thead class="thead-inverse">
	    <tr>
	      <th>Id</th>
	      <th>City</th>
	      <th>State</th>
	      <th>Population</th>
	      <th colspan="3"></th>
	    </tr>
	  </thead>

	  <tbody id="myTable">
	    <% @zips.each do |zip| %>
	      <% zip=toZip(zip) %>
	      <tr>
	        <td scope="row"><%= zip.id %></td>
	        <td><%= zip.city %></td>
	        <td><%= zip.state %></td>
	        <td><%= zip.population %></td>
	        <td><%= link_to 'Show', zip %></td>
	        <td><%= link_to 'Edit', edit_zip_path(zip) %></td>
	        <td><%= link_to 'Destroy', zip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	      </tr>
	    <% end %>
	  </tbody>
	</table>
</div>

	<br>
	<div class="digg_pagination">
    <%= will_paginate @zips, :container => false %>
  </div>
</div>
